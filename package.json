{
  "name": "immutable-diff",
  "version": "0.1.1",
  "description": "Hi-Fi diffing for Immutable.js data structures, forked from immutablediff",
  "main": "lib/diff.js",
  "jsnext:main": "src/diff.js",
  "files": [
    "lib",
    "src",
    "tests",
    "npm-scripts",
    "*.MD"
  ],
  "scripts": {
    "lint": "eslint src tests",
    "build": "rimraf lib && babel ./src -d lib",
    "test": "npm run lint && npm run build && npm run test-node",
    "test-node": "mocha --compilers js:babel-core/register tests/*.test.js",
    "postinstall": "node ./npm-scripts/postinstall.js"
  },
  "keywords": [
    "immutable",
    "diff"
  ],
  "author": [
    "Tim Griesser <tgriesser@gmail.com>",
    "Intelie"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/tgriesser/immutable-diff.git"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.1.18",
    "eslint": "^1.10.1",
    "immutable": "^4.0.0",
    "jscheck": "^0.2.0",
    "mocha": "^2.3.4",
    "rimraf": "^2.4.4"
  },
  "peerDependencies": {
    "immutable": "^4.0.0"
  }
}