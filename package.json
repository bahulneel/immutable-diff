{
  "name": "immutable-diff",
  "version": "1.0.0",
  "description": "Hi-Fi diffing for Immutable.js data structures, forked from immutablediff",
  "main": "lib/diff.js",
  "jsnext:main": "src/diff.js",
  "types": "index.d.ts",
  "exports": {
    "types": "./index.d.ts",
    "default": "./src/diff.js"
  },
  "files": [
    "lib",
    "src",
    "tests",
    "npm-scripts",
    "*.MD",
    "postinstall.js",
    "record-alias-v3.d.ts",
    "record-alias-v4.d.ts"
  ],
  "scripts": {
    "lint": "eslint src tests",
    "build": "rimraf lib && babel ./src -d lib",
    "test": "npm run lint && npm run build && npm run test-node",
    "test-node": "mocha --compilers js:babel-core/register tests/*.test.js",
    "postinstall": "node postinstall.js"
  },
  "keywords": [
    "immutable",
    "diff"
  ],
  "author": "Tim Griesser <tgriesser@gmail.com>",
  "contributors": [
    "Intelie",
    "Bahul Neel Upadhyaya <bahulneel@gmail.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/tgriesser/immutable-diff.git"
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-core": "^6.2.1",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.1.18",
    "eslint": "^1.10.1",
    "immutable": "^3.8.2",
    "jscheck": "^0.2.0",
    "mocha": "^2.3.4",
    "rimraf": "^2.4.4"
  },
  "peerDependencies": {
    "immutable": ">=3 <6"
  },
  "dependencies": {
    "immutable": ">=3 <6"
  }
}